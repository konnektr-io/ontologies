    owl:onProperty <http://www.co-ode.org/ontologies/pizza/pizza.owl#hasSpiciness> ;
    owl:someValuesFrom <http://www.co-ode.org/ontologies/pizza/pizza.owl#Hot>
  ] ;
  owl:disjointWith <http://www.co-ode.org/ontologies/pizza/pizza.owl#RosemaryTopping> ;
